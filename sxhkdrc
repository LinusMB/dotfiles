super + Return
    alacritty

super + space
    dmenu_run -nb '#1e1e1e' -sb '#1e1e1e' -nf '#fff' -sf '#aaa' -fn Overpass:size=14

XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioMicMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioRaiseVolume
    pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioLowerVolume
    pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86MonBrightnessDown
    sudo `which backlight` -dec 0.05

XF86MonBrightnessUp
    sudo `which backlight` -inc 0.05
